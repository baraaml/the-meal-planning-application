{
	"info": {
		"_postman_id": "mealflow-recommendation-service-v1",
		"name": "MealFlow Recommendation Service",
		"description": "Comprehensive API collection for MealFlow Recommendation Service\n\nBase URL: {{protocol}}://{{host}}:{{port}}/api/{{version}}",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Recommendations",
			"item": [
				{
					"name": "User Recommendations",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recommend/user/{{user_id}}?limit={{limit}}&recommendation_type={{recommendation_type}}&cuisine={{cuisine}}&dietary_restriction={{dietary_restriction}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recommend", "user", "{{user_id}}"],
							"query": [
								{"key": "limit", "value": "{{limit}}"},
								{"key": "recommendation_type", "value": "{{recommendation_type}}"},
								{"key": "cuisine", "value": "{{cuisine}}"},
								{"key": "dietary_restriction", "value": "{{dietary_restriction}}"}
							]
						}
					}
				},
				{
					"name": "Similar Recipes",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recommend/similar/{{recipe_id}}?limit={{limit}}&similarity_method={{similarity_method}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recommend", "similar", "{{recipe_id}}"],
							"query": [
								{"key": "limit", "value": "{{limit}}"},
								{"key": "similarity_method", "value": "{{similarity_method}}"}
							]
						}
					}
				},
				{
					"name": "Trending Recipes",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/trending?time_window={{time_window}}&limit={{limit}}&cuisine={{cuisine}}&dietary_restriction={{dietary_restriction}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "trending"],
							"query": [
								{"key": "time_window", "value": "{{time_window}}"},
								{"key": "limit", "value": "{{limit}}"},
								{"key": "cuisine", "value": "{{cuisine}}"},
								{"key": "dietary_restriction", "value": "{{dietary_restriction}}"}
							]
						}
					}
				},
