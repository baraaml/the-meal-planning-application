					"name": "Cuisine Recommendations",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recommend/cuisine/{{cuisine_id}}?limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recommend", "cuisine", "{{cuisine_id}}"],
							"query": [
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				},
				{
					"name": "Dietary Recommendations",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recommend/dietary/{{dietary_restriction}}?limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recommend", "dietary", "{{dietary_restriction}}"],
							"query": [
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				},
				{
					"name": "Regional Recommendations",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recommend/region/{{region}}?limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recommend", "region", "{{region}}"],
							"query": [
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				},
				{
					"name": "Continental Recommendations",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recommend/continent/{{continent}}?limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recommend", "continent", "{{continent}}"],
							"query": [
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				},
				{
					"name": "Calorie Range Recommendations",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recommend/calories?min={{min_calories}}&max={{max_calories}}&limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recommend", "calories"],
							"query": [
								{"key": "min", "value": "{{min_calories}}"},
								{"key": "max", "value": "{{max_calories}}"},
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				},
				{
					"name": "Quick Recipe Recommendations",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recommend/quick?max_time={{max_total_time}}&limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recommend", "quick"],
							"query": [
								{"key": "max_time", "value": "{{max_total_time}}"},
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				}
			]
		},
		{
			"name": "Recipes",
			"item": [
				{
					"name": "Get Recipe by ID",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recipes/{{recipe_id}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recipes", "{{recipe_id}}"]
						}
					}
				},
				{
					"name": "List Recipes",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recipes?page={{page}}&limit={{limit}}&region={{region}}&sub_region={{sub_region}}&dietary_restriction={{dietary_restriction}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recipes"],
							"query": [
								{"key": "page", "value": "{{page}}"},
								{"key": "limit", "value": "{{limit}}"},
								{"key": "region", "value": "{{region}}"},
								{"key": "sub_region", "value": "{{sub_region}}"},
								{"key": "dietary_restriction", "value": "{{dietary_restriction}}"}
							]
						}
					}
				},
				{
					"name": "Create Recipe",
					"request": {
						"method": "POST",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"recipe_title\": \"{{recipe_title}}\",\n    \"region\": \"{{region}}\",\n    \"sub_region\": \"{{sub_region}}\",\n    \"continent\": \"{{continent}}\",\n    \"cook_time\": {{cook_time}},\n    \"prep_time\": {{prep_time}},\n    \"calories\": {{calories}}\n}"
						},
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recipes",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recipes"]
						}
					}
				},
				{
					"name": "Update Recipe",
					"request": {
						"method": "PUT",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"recipe_title\": \"{{recipe_title}}\",\n    \"region\": \"{{region}}\",\n    \"sub_region\": \"{{sub_region}}\"\n}"
						},
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recipes/{{recipe_id}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recipes", "{{recipe_id}}"]
						}
					}
				},
				{
					"name": "Delete Recipe",
					"request": {
						"method": "DELETE",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recipes/{{recipe_id}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recipes", "{{recipe_id}}"]
						}
					}
				},
				{
					"name": "Search Recipes",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recipes/search?query={{search_query}}&page={{page}}&limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recipes", "search"],
							"query": [
								{"key": "query", "value": "{{search_query}}"},
								{"key": "page", "value": "{{page}}"},
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				},
				{
					"name": "Filter Recipes by Calories",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recipes/filter/calories?min={{min_calories}}&max={{max_calories}}&page={{page}}&limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recipes", "filter", "calories"],
							"query": [
								{"key": "min", "value": "{{min_calories}}"},
								{"key": "max", "value": "{{max_calories}}"},
								{"key": "page", "value": "{{page}}"},
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				},
				{
					"name": "Filter Recipes by Time",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/recipes/filter/time?max_total={{max_total_time}}&page={{page}}&limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "recipes", "filter", "time"],
							"query": [
								{"key": "max_total", "value": "{{max_total_time}}"},
								{"key": "page", "value": "{{page}}"},
								{"key": "limit", "value": "{{limit}}"}
							]
						}
					}
				}
			]
		},
		{
			"name": "Interactions",
			"item": [
				{
					"name": "Record Interaction",
					"request": {
						"method": "POST",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"user_id\": \"{{user_id}}\",\n    \"meal_id\": \"{{recipe_id}}\",\n    \"interaction_type\": \"{{interaction_type}}\",\n    \"rating\": {{rating}}\n}"
						},
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/interactions",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
							"path": ["api", "{{version}}", "interactions"]
						}
					}
				},
				{
					"name": "Get User Interactions",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{protocol}}://{{host}}:{{port}}/api/{{version}}/interactions/user/{{user_id}}?interaction_type={{interaction_type}}&limit={{limit}}",
							"protocol": "{{protocol}}",
							"host": ["{{host}}"],
							"port": "{{port}}",
